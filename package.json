    "jest": "^29.0.0",
    "npm": "^8.18.0"
  },
  "exports": {
    "import": "./index.mjs",
    "require": "./index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BillyAleks/lord_of_the_rings_sdk"
  },
  "keywords": ["sdk", "lord-of-the-rings"]
